{
	"_version" : "1.3.0",
	"sap.app" : {
		"_version" : "1.3.0",
		"id"		: "opensap.webide2github",
		"type"		: "application",
		"applicationVersion" : {
			"version" : "1.0.0"
		}
	},
	"sap.ui" : {
		"_version" : "1.3.0",
		"technology" : "UI5",
		"deviceTypes" : {
			"desktop" : true,
			"tablet" : true,
			"phone" : true
		},
		"supportedThemes" : ["sap_belize"]
	},
	"sap.ui5" : {
		"_version" : "1.2.0",
		"rootView" : {
			"viewName" : "opensap.webide2github.view.App",
			"type"	: "XML",
			"id"	: "app"
		},
		"handleValidation": true,
		"autoPrefixId" : true,
		"dependencies" : {
			"minUI5Version" : "1.34",
			"libs" : {
				"sap.ui.core" : {
					"minVersion" : "1.34.0"
				},
				"sap.m" : {
					"minVersion" : "1.34.0"
				},	
				"sap.ui.layout" : {
					"minVersion" : "1.34.0"
				}			
			}
		},
		"contentDensities" : {
			"compact" : true,
			"cozy"	: true
		}
	}
}